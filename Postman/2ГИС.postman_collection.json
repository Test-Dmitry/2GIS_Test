					"// Проверяем имя запроса\r",
					"if (pm.info.requestName === \"Smoke Без токена\") {\r",
					"    // Пропускаем выполнение скрипта для этого запроса\r",
					"    return;\r",
					"}\r",
					"\r",
					"// Pre-request Script для /v1/favorites\r",
					"pm.sendRequest({\r",
					"    url: \"https://regions-test.2gis.com/v1/auth/tokens\",\r",
					"    method: \"POST\"\r",
					"}, function (err, res) {\r",
					"    if (err) throw err;\r",
					"\r",
					"    let setCookieHeader = res.headers.get(\"Set-Cookie\");\r",
					"    let tokenMatch = setCookieHeader.match(/token=([^;]+);/);\r",
					"    if (tokenMatch) {\r",
					"        let tokenValue = tokenMatch[1];\r",
					"\r",
					"        // Добавляем Cookie Header в текущий запрос\r",
					"        pm.request.headers.upsert({\r",
					"            key: \"Cookie\",\r",
					"            value: \"token=\" + tokenValue\r",
					"        });\r",
					"\r",
					"        console.log(\"Token set:\", tokenValue);\r",
					"    }\r",
					"});"
				]
			}
